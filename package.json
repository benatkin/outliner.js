{
  "author": "Ben Atkin <ben@benatkin.com> (http://benatkin.com/)",
  "name": "outliner",
  "description": "a couchapp for displaying data in an outline format",
  "version": "0.0.0",
  "homepage": "http://outlinerjs.com/",
  "repository": {
    "type": "git",
    "url": "git://github.com/benatkin/outliner.js.git"
  },
  "main": "lib/outliner",
  "engines": {
    "node": ">=v0.4.8"
  },
  "scripts": {
    "push":       "couchapp push app.js $COUCH_URL",
    "push-redir": "couchapp push redir.js $COUCH_URL",
    "watch":      "couchapp sync app.js $COUCH_URL || true",
    "build-app":  "node build-app.js"
  },
  "dependencies": {
    "couchapp":   "*",
    "underscore": "*",
    "browserify": "*",
    "shimify":    "*",
    "seq":        "*",
    "traverse":   "*",
    "hashish":    "*",
    "chainsaw":   "*"
  },
  "devDependencies": {}
}
